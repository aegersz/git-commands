git init
echo
git config --global user.name "Your Name"
git config --global user.email "yournme@somemail.com"
git config --list
echo
#echo "# git-commands" >> README.md
git add README.md
git add file1
git add dir/file2
echo
git status
echo 
git status -s
echo
git commit -m new
git remote add origin https://github.com/yourname/yourrepo.git
git push -u origin master
echo
git status
echo
git log

